{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aks-bc-003"
		},
		"aks-synapse-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'aks-synapse-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:aks-synapse.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"ls_backend_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_backend'"
		},
		"ls_raw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_raw'"
		},
		"ls_syn_dedi_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_syn_dedi'"
		},
		"ls_synapse_dedi_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_synapse_dedi'"
		},
		"aks-synapse-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://std5.dfs.core.windows.net"
		},
		"ls_backend_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://std5.dfs.core.windows.net/"
		},
		"ls_raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://std5.dfs.core.windows.net/"
		},
		"ls_syn_dedi_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "aks-synapse.sql.azuresynapse.net"
		},
		"ls_syn_dedi_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "aksstagging"
		},
		"ls_syn_dedi_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Aditya-98"
		},
		"ls_synapse_dedi_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "aks-synapse.sql.azuresynapse.net"
		},
		"ls_synapse_dedi_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "aksstagging"
		},
		"ls_synapse_dedi_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Aditya-98"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aks-bc-003_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_backend_accountKey": {
						"value": "[parameters('ls_backend_accountKey')]"
					},
					"ls_raw_accountKey": {
						"value": "[parameters('ls_raw_accountKey')]"
					},
					"ls_syn_dedi_password": {
						"value": "[parameters('ls_syn_dedi_password')]"
					},
					"ls_synapse_dedi_password": {
						"value": "[parameters('ls_synapse_dedi_password')]"
					},
					"aks-synapse-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('aks-synapse-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"ls_backend_properties_typeProperties_url": {
						"value": "[parameters('ls_backend_properties_typeProperties_url')]"
					},
					"ls_raw_properties_typeProperties_url": {
						"value": "[parameters('ls_raw_properties_typeProperties_url')]"
					},
					"ls_syn_dedi_properties_typeProperties_server": {
						"value": "[parameters('ls_syn_dedi_properties_typeProperties_server')]"
					},
					"ls_syn_dedi_properties_typeProperties_database": {
						"value": "[parameters('ls_syn_dedi_properties_typeProperties_database')]"
					},
					"ls_syn_dedi_properties_typeProperties_userName": {
						"value": "[parameters('ls_syn_dedi_properties_typeProperties_userName')]"
					},
					"ls_synapse_dedi_properties_typeProperties_server": {
						"value": "[parameters('ls_synapse_dedi_properties_typeProperties_server')]"
					},
					"ls_synapse_dedi_properties_typeProperties_database": {
						"value": "[parameters('ls_synapse_dedi_properties_typeProperties_database')]"
					},
					"ls_synapse_dedi_properties_typeProperties_userName": {
						"value": "[parameters('ls_synapse_dedi_properties_typeProperties_userName')]"
					},
					"aks-synapse-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('aks-synapse-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}